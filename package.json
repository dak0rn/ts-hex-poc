    "@types/ioredis": "^4.17.10",
    "@types/node": "^14.14.10",
    "@types/sinon": "^9.0.9",
    "ava": "^3.13.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "ini": "^1.3.5",
    "ioredis": "^4.19.4",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^9.2.1",
    "source-map-support": "^0.5.19",
